{
"manifest_version":2,
"name":"ReMinD",
"version":"1.0",
"description":"Keep a reminder on your net surfing!",
    "icons":{
        "16":"images/icon16.png",
        "48":"images/icon48.png",
        "128":"images/icon128.png"    
    },
    
    "browser_action":{
        "default_icon":"images/icon16.png",
        "default_popup":"popup.html"
    },
    
    
    "permissions":[
        "storage",
        "notifications"
    ]
}